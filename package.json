  "description": "",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "allure-commandline": "^2.29.0",
    "cypress": "^13.13.0",
    "cypress-iframe": "^1.0.1",
    "cypress-xpath": "^2.0.1",
    "mocha-allure-reporter": "^1.4.0",
    "rimraf": "^5.0.8"
  },
  "dependencies": {
    "cypress-real-events": "^1.12.0"
  }
}
